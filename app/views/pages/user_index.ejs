<!-- views/pages/index.ejs -->

<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../partials/head %>
</head>

<body class="container">

    <header>
        <% include ../partials/header %>
    </header>

    <main>
        <div class="jumbotron">
            <p>
                <% users.forEach(function(re) { %>
                <li><%= re.fullname %> - <%= re.email %> - <a href="user/delete/<%= re._id %>">Delete</a> - <a
                        href="user/edit/<%= re._id %>">Edit</a></li>
                <% }); %>
            </p>
            <% if (typeof message != 'undefined') { %>
            <p><%= message %></p>
            <% } %>
            <a href="/register">Add more Account</a>
        </div>
    </main>

    <footer>
        <% include ../partials/footer %>
    </footer>

</body>

</html>